<form>
	
	<fieldset addfieldpath="/libraries/concan/fields">
		
		<field name = "id" type = "hidden"/>
		
		<field name = "nombre" type = "text"
			label		= "Material"
			description	= "Nombre del Material"			
			required	= "true"		
			class		= "form-control"
			labelclass	= "form-label"
		/>
		
				
		
				
		<field name = "descripcion" type = "textarea"
			label		= "Descripción"
			description	= "Descripción"			
			required	= "true"	
			class		= "form-control"
			labelclass	= "form-label"	
			hint		= "Descripción"
		/>		
		
	</fieldset>
	
</form>